{
  "name": "threatdatascience/php-http-actions",
  "type": "library",
  "version": "1.0.0",
  "description": "a very minimal helper class for writing RAD PSR-7 PHP applications",
  "authors": [
    {
      "name": "Andrew Breksa",
      "email": "andrew@threatdatascience.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "ThreatDataScience\\HttpActions\\": "src/"
    }
  },
  "require": {
    "php": "^7.2",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^1.6",
    "suin/json": "^1.2",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.4",
    "mockery/mockery": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
